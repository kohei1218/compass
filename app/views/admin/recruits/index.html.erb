<p id="notice"><%= notice %></p>

<h1>Admin Recruits</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_recruits.each do |admin_recruit| %>
      <tr>
        <td><%= link_to 'Show', admin_recruit %></td>
        <td><%= link_to 'Edit', edit_admin_recruit_path(admin_recruit) %></td>
        <td><%= link_to 'Destroy', admin_recruit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Recruit', new_admin_recruit_path %>
